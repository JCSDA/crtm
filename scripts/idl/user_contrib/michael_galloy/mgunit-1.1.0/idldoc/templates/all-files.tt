<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
 "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<!-- Generated by IDLdoc [% idldoc_version %] on [% date %] -->

<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
  <head>
    <meta http-equiv="Content-Type" content="text/html;charset=[% charset %]"/>
    <title>All files ([% title %])</title>

    [% IF embed %]
    <style type="text/css" media="all">
      [% INCLUDE listing_css_location %]
    </style>
    <style type="text/css" media="print">
      [% INCLUDE print_listing_css_location %]
    </style>
    [% ELSE %]
    <link rel="stylesheet" type="text/css" media="all" 
          href="[% relative_root %]idldoc-resources/listing.css" />
    <link rel="stylesheet" type="text/css" media="print" 
          href="[% relative_root %]idldoc-resources/listing-print.css" />
    [% END %]
  </head>
  
  <body>
    <h1>All files</h1>
  
  	[% IF n_visible_pro_files gt 0 %]
    <h1>.pro files &nbsp;&nbsp; <span>[% n_visible_pro_files %] files</span></h1>
    	<ul>
    		[% FOREACH f IN visible_pro_files %]
    		  <li>[% SCOPE f %]<a href="[% url %][% local_url %]" target="main_frame">[% basename %]</a>[% END %]</li>
    		[% END %]
    	</ul>
    [% END %]

    [% IF n_visible_dlm_files gt 0 %]
    <h1>.dlm files &nbsp;&nbsp; <span>[% n_visible_dlm_files %] files</span></h1>
      <ul>
        [% FOREACH f IN visible_dlm_files %]
          <li>[% SCOPE f %]<a href="[% url %][% local_url %]" target="main_frame">[% basename %]</a>[% END %]</li>
        [% END %]
      </ul>    
    [% END %]
        
    [% IF n_visible_sav_files gt 0 %]
    <h1>.sav files &nbsp;&nbsp; <span>[% n_visible_sav_files %] files</span></h1>
      <ul>
        [% FOREACH f IN visible_sav_files %]
          <li>[% SCOPE f %]<a href="[% url %][% local_url %]" target="main_frame">[% basename %]</a>[% END %]</li>
        [% END %]
      </ul>    
    [% END %]
    
    [% IF n_visible_idldoc_files %]
    <h1>.idldoc files &nbsp;&nbsp; <span>[% n_visible_idldoc_files %] files</span></h1>
      <ul>
        [% FOREACH f IN visible_idldoc_files %]
          <li>[% SCOPE f %]<a href="[% url %][% local_url %]" target="main_frame">[% file_title %]</a>[% END %]</li>
        [% END %]
      </ul>     
    [% END %]
            
  </body>
</html>
