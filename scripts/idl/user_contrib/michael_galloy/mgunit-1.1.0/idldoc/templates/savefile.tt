<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
 "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<!-- Generated by IDLdoc [% idldoc_version %] on [% date %] -->

<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
  <head>
    <meta http-equiv="Content-Type" content="text/html;charset=[% charset %]"/>
    <title>[% basename %] ([% title %])</title>

    [% IF embed %]
    <style type="text/css" media="all">
      [% INCLUDE css_location %]
    </style>
    <style type="text/css" media="print">
      [% INCLUDE print_css_location %]
    </style>
    [% ELSE %]
    <link rel="stylesheet" type="text/css" media="all" 
          href="[% relative_root %]idldoc-resources/main.css" />
    <link rel="stylesheet" type="text/css" media="print" 
          href="[% relative_root %]idldoc-resources/main-print.css" />
    [% END %]
    
    <script type="text/javascript">
      function setTitle() {
        parent.document.title="[% basename %] ([% title %])";
      }
    </script>
  </head>
    
  <body onload="setTitle();">

    [% INCLUDE_TEMPLATE idldoc_header_location %]
    
    <div class="content">
      <h2 class="directory">[% location %]</h2>
      <h1 class="basename">[% basename %]</h1>
            
      <h2>SAV file attributes</h2>
      
      <table class="attribute smaller" summary="SAV file attributes">  
        <tr>
          <td class="name">Description:</td>
          <td>[% description %]</td>
        </tr>    
        <tr>
          <td class="name">SAV file type:</td>
          <td>[% filetype %]</td>
        </tr>
        <tr>
          <td class="name">Creator: </td>
          <td>[% user %]@[% host %] on [% os %]/[% arch %]</td>
        </tr>
        <tr>
          <td class="name">Creation date: </td>
          <td>[% creation_date %]</td>
        </tr>      
        <tr>
          <td class="name">IDL version: </td>
          <td>[% release %]</td>
        </tr>    
      </table>

      <h2>Contents</h2>
      
      <table class="attribute smaller" summary="Contents of SAV file">      
        <tr>
          <td class="name">[% IF n_procedure gt 0%]<a href="#procedures">[% END %]Procedures:[% IF n_procedure gt 0 %]</a>[% END %]</td>
          <td>[% n_procedure %]</td>
        </tr>  
        
        <tr>
          <td class="name">[% IF n_function gt 0 %]<a href="#functions">[% END %]Functions:[% IF n_function gt 0 %]</a>[% END %]</td>
          <td>[% n_function %]</td>
        </tr> 
        
        <tr>
          <td class="name">[% IF n_var gt 0 %]<a href="#vars">[% END %]Variables:[% IF n_var gt 0 %]</a>[% END %]</td>
          <td>[% n_var %]</td>
        </tr>  
        
        <tr>
          <td class="name">[% IF n_sysvar gt 0 %]<a href="#system">[% END %]System variables:[% IF n_sysvar gt 0 %]</a>[% END %]</td>
          <td>[% n_sysvar %]</td>
        </tr>  
        
        <tr>
          <td class="name">[% IF n_common gt 0 %]<a href="#common">[% END %]Common blocks:[% IF n_common gt 0 %]</a>[% END %]</td>
          <td>[% n_common %]</td>
        </tr>
        
        <tr>
          <td class="name">[% IF n_structdef gt 0 %]<a href="#structures">[% END %]Structure definitions:[% IF n_structdef gt 0 %]</a>[% END %]</td>
          <td>[% n_structdef %]</td>
        </tr>  
        
        <tr>
          <td class="name">[% IF n_pointer_heapvar gt 0 %]<a href="#pointers">[% END %]Pointers:[% IF n_pointer_heapvar gt 0 %]</a>[% END %]</td>
          <td>[% n_pointer_heapvar %]</td>
        </tr>  
                                  
        <tr>
          <td class="name">[% IF n_object_heapvar gt 0 %]<a href="#objects">[% END %]Objects:[% IF n_object_heapvar gt 0 %]</a>[% END %]</td>
          <td>[% n_object_heapvar %]</td>
        </tr>  
      </table>
            
      [% IF n_procedure gt 0 %]
      <div id="procedures">
        <h3>Procedures</h3>
        
        <ul class="smaller">
          [% FOREACH p IN procedures %]
          <li>[% p %]</li>
          [% END %]
        </ul>
      </div>
      [% END %]

      [% IF n_function gt 0 %]
      <div id="functions">
        <h3>Functions</h3>
        
        <ul class="smaller">
          [% FOREACH f IN functions %]
          <li>[% f %]</li>
          [% END %]
        </ul>
      </div>
      [% END %]
            
      [% IF n_var gt 0 %]
      <div id="vars">
        <h3>Variables</h3>
        
        <table class="smaller" summary="Variables">
        [% FOREACH v IN variables %][% SCOPE v %]
          <tr>
            <td>[% IF has_thumbnail %]<img src="[% thumbnail_url %]" alt="[thumbnail]"/>[% END %]</td>
            <td>[% name %]</td>
            <td><code>[% declaration %]</code></td>             
          </tr>
        [% END %][% END %]
        </table>
      </div>
      [% END %]

      [% IF n_sysvar gt 0 %]
      <div id="system">
        <h3>System variables</h3>
        
        <table class="smaller" summary="System variables">
        [% FOREACH sv IN system_variables %][% SCOPE sv %]
          <tr>
            <td>[% IF has_thumbnail %]<img src="[% thumbnail_url %]" alt="[thumbnail]"/>[% END %]</td>
            <td>[% name %]</td>
            <td><code>[% declaration %]</code></td>             
          </tr>
        [% END %][% END %]
        </table>
      </div>
      [% END %]
      
      [% IF n_common gt 0 %]
      <div id="common">
        <h3>Common blocks</h3>
        
        <table class="smaller" summary="Common blocks">
        [% FOREACH cb IN common_blocks %][% SCOPE cb %]
          <tr>
            <td>[% name %]</td>
            <td><code>[% declaration %]</code></td>             
          </tr>
        [% END %][% END %]
        </table>
      </div>
      [% END %]
      
      [% IF n_structdef gt 0 %]
      <div id="structures">
        <h3>Structure definitions</h3>
        
        <table class="smaller" summary="Structure definitions">
        [% FOREACH s IN structure_definitions %][% SCOPE s %]
          <tr>
            <td>[% name %]</td>
            <td><code>[% declaration %]</code></td>             
          </tr>
        [% END %][% END %]
        </table>
      </div>
      [% END %]
      
      [% IF n_pointer_heapvar gt 0 %]
      <div id="pointers">
        <h3>Pointers</h3>
        
        <table class="smaller" summary="Pointers">
        [% FOREACH p IN pointers %][% SCOPE p %]
          <tr>
            <td>[% IF has_thumbnail %]<img src="[% thumbnail_url %]" alt="[thumbnail]"/>[% END %]</td>
            <td>[% name %]</td>
            <td><code>[% declaration %]</code></td>             
          </tr>
        [% END %][% END %]
        </table>
      </div>
      [% END %]
      
      [% IF n_object_heapvar gt 0 %]
      <div id="objects">
        <h3>Objects</h3>
        
        <table class="smaller" summary="Objects">
        [% FOREACH o IN objects %][% SCOPE o %]
          <tr>
            <td>[% name %]</td>
            <td><code>[% declaration %]</code></td>             
          </tr>
        [% END %][% END %]
        </table>
      </div>
      [% END %]
                  
      <h2>File attributes</h2>
      
        <table class="attribute smaller" summary="File attributes">
          <tr>
            <td class="name">Modification date:</td>
            <td>[% modification_time %]</td>
          </tr>

          <tr>
            <td class="name">Size:</td>
            <td>[% size %]</td>
          </tr>          
        </table>   
        
      [% IF footer ne '' %][% INCLUDE_TEMPLATE footer %][% END %]
    </div>
    
    [% INCLUDE_TEMPLATE idldoc_footer_location %]    
  </body>
</html>
 